<%- include('partials/header') -%>
<!--BODY-->
<section class="text-gray-900 bg-ante-400 body-font h-full md:h-screen">
  <!--CONTAINER-->
  <div
    class="container mx-auto flex px-5 py-24 md:flex-row flex-col items-center">
    <!--LOGO-->
    <div class="lg:max-w-lg lg:w-full md:w-1/2 w-5/6 md:mb-0 mb-10">
      <img
        class="object-cover object-center rounded"
        alt="hero"
        src="/images/logo-index.jpg"
      />
    </div>
    <!--/LOGO-->
    <!--CONTENT-->
    <div
      class="lg:flex-grow md:w-1/2 lg:pl-24 md:pl-16 flex flex-col md:items-start md:text-left items-center text-center"
    >
      <h1 class="title-font sm:text-4xl text-3xl mb-4 font-medium text-white">
        Everyone pitches in.
      </h1>
      <p class="mb-8 leading-relaxed">
      </p>
      <!--NAV-->
      <div class="flex justify-center">
        <a href="/login" class="inline-flex text-white bg-violet-600 border-0 py-2 px-6 focus:outline-none hover:bg-violet-500 rounded text-lg">
          Login
        </a>
        <a href="/signup" class="ml-4 inline-flex text-gray-400 bg-slate-800 border-0 py-2 px-6 focus:outline-none hover:bg-slate-700 hover:text-white rounded text-lg"
        >
          Sign Up
      </a>
      </div>
      <!--/NAV-->
    </div>
    <!--/CONTENT-->
  </div>
  <!--/CONTAINER-->
</section>
<!--/BODY-->
<%- include('partials/footer') -%>
